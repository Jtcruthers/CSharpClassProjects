<Window x:Class="CarruthersFav5Web.MyBrowser"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CarruthersFav5Web"
        mc:Ignorable="d"
        Title="MyBrowser" Height="800" Width="1024">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
            <RowDefinition Height="80"/>
        </Grid.RowDefinitions>
        <WebBrowser x:Name="controlWebBrowser" Margin="10,10,9.6,0.4" VerticalAlignment="Stretch"/>
        <Button Content="Go" HorizontalAlignment="Right" Margin="0,30,0,0" Grid.Row="1" VerticalAlignment="Center" Width="75" Click="Button_Click"/>
        <ComboBox x:Name="comboFavoriteLinks" HorizontalAlignment="Left" Margin="10,27.6,0,0" Grid.Row="1" VerticalAlignment="Top" Width="786" Height="37" SelectionChanged="ComboBox_SelectionChanged">
            <ComboBoxItem Content="https://www.google.com" HorizontalAlignment="Left" Width="784"/>
            <ComboBoxItem x:Name="Test" Content="https://www.reddit.com" HorizontalAlignment="Left" Width="784"/>
        </ComboBox>
    </Grid>
</Window>
